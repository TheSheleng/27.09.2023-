<Window x:Class="_27._09._2023_Команды.NewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_27._09._2023_Команды"
        mc:Ignorable="d"
        Title="NewWindow" Height="110" Width="300" WindowStartupLocation="CenterScreen" >
    <Grid Background="Coral">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <TextBlock Foreground="White" Text="Key:" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10"/>
        <TextBox Foreground="White" BorderBrush="White" Background="Coral"  x:Name="textBox" Grid.Column="1" Margin="10" Height="20" VerticalAlignment="Top"/>
        
        <StackPanel Orientation="Vertical" Grid.ColumnSpan="2" Margin="0,30,0,0">
            <Button Foreground="White" BorderBrush="White" Background="Coral" Content="Register" Height="25" Width="280" Margin="5" Click="Button_Click"/>
            <Button Foreground="White" BorderBrush="White" Background="Coral" Content="Cancel" Height="25" Width="280"  Click="Click_Close"/>
        </StackPanel>
    </Grid>
</Window>
